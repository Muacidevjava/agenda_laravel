*{
	padding:0;
	margin:0;
	box-sizing:border-box;
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	-o-box-sizing:border-box;
}

body{font-family:arial, 'sans-serif'; font-size:1em; background:#080808;padding-top:4.5rem}
	a{text-decoration:none;}
	a:hover{text-decoration:none;}

.site{height:100%;width:100%;}
.site:before,.site:after
{content:"";height:100%;width:100%;}
.menu_lateral{
	width: auto;
    background-color: #080808;
    border-radius: 0;
    border-right: solid 2px #858585;
    position: fixed;
    left: 0;
    top: 3rem;
    bottom: 0;	
}
.topo .cabecalho .titulo{
    height: 64px;
    border-left:0;
    /*border-bottom: solid 2px #0098ac;*/
	position:fixed;top:0;left:0;right:0;z-index:4
}
.topo .cabecalho .titulo .user{	display:block;  width: 65px;   height: 40px;}
.topo .cabecalho .titulo .user li{position:relative;display:inline-block}
.topo .cabecalho .titulo .user li.cx-user img{width:40px;height:40px}
.topo .cabecalho .titulo .user li.cx-user ul{display:none;background: #000;  border: solid 2px #858585;  position: absolute;   width: 200px;   right: 0;  z-index: 10000;border-radius:5px}
.topo .cabecalho .titulo .user li.cx-user ul li{padding:10px;text-align:left;display:block;    border-bottom: solid 2px #dddddd78;}
.topo .cabecalho .titulo .user li.cx-user ul li:last-child{border-bottom:0}
.topo .cabecalho .titulo .user li.cx-user ul li a{display:block}
.topo .cabecalho .titulo .user li.cx-user:hover ul{display:block}


.menu_lateral .menu{display: block;margin-top:4rem}
.menu_lateral .mobmenu{display:none}
.menu_lateral .menu li{display: block;padding:7px 0}
.menu_lateral .menu li a{display: flex; align-items: center;  gap: 10px;  font-weight:600; padding: 10px 2rem;color:#fff;transition:all .4s linear}
.menu_lateral .menu li a:hover{transition:all .4s linear;opacity: .6; color:#ddd}
.menu_lateral .menu li svg{    display: block;  stroke: #ffffff;  color: #0098ac;  width: 2.3rem;  height: 2.3rem;}
.Conteudo{    margin: 0 auto;  width: 1116px;  max-width: 100%;  margin-left: 310px;}
.Conteudo.cabecalho .titulo{background-color: #dd491b!important;justify-content:space-between!important}

.titulo{height: 50px;background-color:#080808;width:100%;border-left:solid 1px #858585;display: flex;justify-content:start;align-items: center; border-bottom: solid 1px #ddd;}
.titulo span{text-transform: uppercase;padding:.5rem 1.8rem;color:#fff;letter-spacing: 1px}
.titulo span:not(span:first-child){border-left:solid 1px #ddd}
.titulo a:hover{opacity:.6;transition:all .4s linear}
.titulo a{transition:all .4s linear;    color: #fff!important;}
.user-top{transition: all .4s linear;margin-right:1rem;display:flex;align-items: center;color:#b5b4b4;position: absolute;}
.user-top .user{width:32px;height:32px;margin-right:5px;display:flex;align-items: center;justify-content: center;}

.logomarca{width:100%;display: flex;align-items: center;justify-content:space-between!important;  padding-left: 2rem;}
.dataTables_length span{color:#fff}

.titulo.border-w-2{text-align:left;border-width:2px!important;border-bottom:0!important;border-radius:13px 13px 0 0; border-color: #858585!important;}
.titulo.completo{	border:solid 2px #858585!important;border-radius:15px!important}
.titulo.completo svg{stroke:#ffffff}
.base{margin:0;}
.caixa{background-color: #080808;   border-radius: 18px;  border: solid 2px #858585;color:#fff}
.caixa.h-full{min-height:calc(100vh - 66px)}
.cx-user{display:block;align-items: center;text-align:center}
.cx-user img{width: 80px;  height: 80px;  border-radius: 50px; margin-right: 10px;  border: solid 2px #fff;}
.cx-user h5{font-size:1rem;color:#e1e0e0}
.cx-user small:nth-of-type(1){border-left:0;display:inline-block;padding-left:0}
.cx-user small{display:inline-block;padding:5px 10px;color:#e1e0e0}
.caixa-form.p-3{padding-left:3rem!important;padding-right:3rem!important}
.progresso{display: block;padding:1rem }
.progresso small{display: block;}
/* For Firefox */
.progresso progress::-moz-progress-bar {
	background: #0098ac;width:100%;border-radius:10px;-webkit-appearance: none;
}

/* For Chrome or Safari */
.progresso progress::-webkit-progress-value {
	background:#0098ac;width:100%;width:100%;border-radius:10px;-webkit-appearance: none;
}
.progresso progress::-webkit-progress-bar {   background: #ccc9;}

/* For IE10 */
.progresso progress {
	width: 100%;
    border-radius: 20px;
    overflow: hidden;
    -webkit-appearance: none;
    border: solid 2px #0098ac;
    height: 22px;
}

#progress {
  width: 100%;
  background-color: #ddd;
    border-radius: 20px;
    border: solid 2px #0098ac;
}

#progbar {
  text-align: right;
  color: #000000;
  height: 15px;
  width: 0px;
  background:#0098ac;
    border-radius: 20px;
	    display: flex;
    justify-content: end;
    align-items: center;
    padding-right: 10px;
    font-weight: 700;
	transition:all 1s linear
}
.grupo-icon{position:relative;width:2.3rem;height:2.3rem;margin:0 auto}
.grupo-icon svg{stroke:#fff}
.grupo-icon .w-6{width:2rem;height:2rem}
.grupo-icon .chec{position:absolute; bottom: -1px; right: -14px;fill: #fff;   stroke: #000;}
.grupo-icon .cloud{position:absolute;left: 3px;fill:#0098ac;fill:none; stroke: #fff;}


.bg-laranja1{ border-color: #858585;  color: #0098ac;  background: #ffffff21;}
.bg-laranja2{ border-color:rgb(255, 81, 0);color:rgb(255, 81, 0)}
.bg-laranja3{border-color:red;color:red}
.text-label {
    color: #dad8d8!important;
}

.tabela{width:100%}
.tabela th,
.tabela td
{font-size:.8rem;padding:.8rem}
.tabela tbody tr:nth-of-type(1n+0){background-color: #141519;}
.tabela tbody tr:nth-of-type(2n+0){background-color: #141519;}
.tabela thead th{border-bottom: solid 2px #666;}
.tabela td{border-bottom:solid 1px #66666670;}

.mostraFiltro{display:none}

.paginacao{display:flex;justify-content: space-between;align-items: center;margin-top:1rem}
.paginacao span p{color:#444;font-size:.9rem}
.paginacao ul {display: flex;gap:5px}
.paginacao ul li {display: block;font-size: .8rem;}

.paginacao ul li a{display: block;border:solid 1px #ddd;border-radius:4px;padding:.6rem .8rem;color:#444}
.paginacao ul li a:hover{background-color: #e8e8e8;}
.paginacao ul li.ativo{display: block;border:solid 1px #dd491b;border-radius:4px;padding:.6rem .8rem;color:#dd491b}

.topo .mobmenu{
	display:none!important;position:fixed;left:15px;top:18px;color:#fff;font-size:1.6rem;z-index:100
}

.login{background:#080808;
	display:flex;justify-content: center;align-items: center;    height: 100vh;}
.login .contain{width:100%;max-width: 920px; margin: 0 auto;}
.login .form-campo{color:#fff; background-color: #070707;  height: 50px;  border: solid 2px #fcfcfc;  padding-left: 45px;  border-radius: 50px;  box-shadow: none;}
.login .form-campo::placeholder{color:#fff}
.login .form-campo svg{}
.login .col-12 svg{position:absolute;left:30px;top:10px;stroke:#fff!important}
.login .col-12 .btn{border-radius:50px;font-size:1.2rem}
.login .caixa{padding:3rem 2rem;border: solid 2px;}




@media (min-width:1706px){
	.Conteudo {  margin: 0 auto;   width: 1165px;   max-width: 100%;   margin-left: 414px;}
	.menu_lateral {  width: 302px;}
}

@media (min-width:1906px){
	.Conteudo {  width: 1378px;margin-left: 462px;}
}

@media (min-width:2206px){
	.menu_lateral {  width: 358px;}
	.Conteudo {    width: 1503px;    margin-left: 555px;}
}

@media (max-width:1160px){
	.Conteudo {   max-width: 100%;  margin-left: 339px;}
}

@media (max-width:992px){
	.topo .mobmenu{display:block!important}
	.menu_lateral{display:none;    position: fixed;   width: 253px;   top: 0;    left: 0;    z-index: 100;    border-radius: 0;
}
	.Conteudo {margin-left: 0;padding:0 1rem}
	
	.site {  width: auto;}
.menu_lateral .mobmenu{display:block}
.logomarca { padding-left: 3rem;}
}


@media (max-width:600px){
	.titulo span {	padding: 0.5rem 0.8rem;	letter-spacing: 0px;font-size: .8rem;}
	.titulo small {	display:none!important}
	.login .col-6.alt{padding-left:30px}
	.caixa-form.p-3 {
    padding-left: 1.4rem!important;
    padding-right: 1.4rem!important;
}
}
